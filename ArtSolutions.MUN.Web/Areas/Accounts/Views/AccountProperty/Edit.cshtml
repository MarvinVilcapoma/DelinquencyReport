@model ArtSolutions.MUN.Web.Areas.Accounts.Models.AccountPropertyModel

@using LocalResource = ArtSolutions.MUN.Web.Resources.AccountProperty
@using GlobalResource = ArtSolutions.MUN.Web.Resources.Global
@using AccountResource = ArtSolutions.MUN.Web.Resources.Account

@{
    /**/

    ViewBag.Title = ViewBag.IsCopyMode == true ? LocalResource.CopyAccountProperty : LocalResource.EditAccountProperty;
}

@using (Ajax.BeginForm("Edit", "AccountProperty", new AjaxOptions { HttpMethod = "POST", OnSuccess = "UserSuccessCallback(data)" }, new { role = "form", id = "form", data_ajax_mode = "replace-with" }))
{
    <div class="row wrapper border-bottom white-bg page-heading">
        <div class="col-sm-12">
            <h2>@GlobalResource.AccountProperty</h2>
            <ol class="breadcrumb">
                <li>
                    <a href="@Url.Action("Index", "Home", new { area = "" })">@GlobalResource.Home</a>
                </li>
                <li>
                    <a href="@Url.Action("List", "AccountProperty", new { area = "Accounts" })">@LocalResource.ListTitle</a>
                </li>
                <li class="active">
                    <strong>@ViewBag.Title</strong>
                </li>
            </ol>
        </div>
    </div>
    @Html.HiddenFor(x => x.OwnerID, new { @id = "hfOwnerID" })
    @Html.HiddenFor(x => x.AccountRowVersion)
    @Html.HiddenFor(x => x.RegisterNumber)
    @Html.HiddenFor(x => x.AccountPropertyConstructionDetailJson)

    @Html.Hidden("hfDateOfMovement")

    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title ibox-title-with-button">
                        <h5>
                            @Html.Raw(ViewBag.IsCopyMode == true ? @LocalResource.CopyAccountProperty : @LocalResource.EditAccountProperty)
                        </h5>
                        <div class="ibox-tools">
                            @if (ViewBag.IsCopyMode == true)
                            {
                                <button id="btnSave" class="btn btn-primary" type="submit" name="actionType" value="2" tabindex=70>@GlobalResource.Copy</button>
                            }
                            else
                            {
                                <button id="btnViewHistory" class="btn btn-info" type="button" tabindex=72>@LocalResource.PropertyFilingHistory</button>
                                <button id="btnSave" class="btn btn-primary" type="submit" name="actionType" value="1" tabindex=70>@GlobalResource.Update</button>
                            }
                            <button class="btn btn-white" type="button" onclick="resolveRedirectURL(3)" tabindex=71>@GlobalResource.Cancel</button>
                        </div>
                    </div>
                    <div class="ibox-content">
                        <div class="project-list form-horizontal">

                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@LocalResource.PropertyNumber <span class="text-danger">*</span></label>
                                <div class="col-sm-4">
                                    @if (ViewBag.IsCopyMode == true)
                                    {
                                        @Html.TextBoxFor(d => d.PropertyNumber, new { @class = "form-control required", autofocus = "autofocus", maxlength = 10, tabindex = 1 })
                                    }
                                    else
                                    {
                                        @Html.TextBoxFor(d => d.PropertyNumber, new { @class = "form-control required", autofocus = "autofocus", @readonly = true, maxlength = 10, tabindex = 1 })
                                    }
                                    @Html.ValidationMessageFor(d => d.PropertyNumber)
                                    @Html.HiddenFor(d => d.ID)
                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.CondoType</label>
                                <div class="col-sm-4">
                                    @Html.DropDownListFor(d => d.CondoTypeID, Model.CondoTypeList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlCondoType", tabindex = 2 })
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@LocalResource.Condo</label>
                                <div class="col-sm-4">
                                    @Html.DropDownListFor(d => d.CondoID, Model.CondoList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlCondo", tabindex = 3 })
                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.RightCode <span class="text-danger">*</span></label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.RigthNumber, new { @class = "form-control required", maxlength = 10, tabindex = 4 })
                                    @Html.ValidationMessageFor(d => d.RigthNumber)
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@LocalResource.Owner <span class="text-danger">*</span></label>
                                <div class="col-sm-4">
                                    @if (ViewBag.IsCopyMode == true)
                                    {
                                        @Html.DropDownListFor(d => d.OwnerID, Model.AccountList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown required", tabindex = 5 })
                                    }
                                    else
                                    {
                                        @Html.DropDownListFor(d => d.OwnerID, Model.AccountList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown required", @disabled = "disabled", tabindex = 5 })
                                    }

                                    @Html.ValidationMessageFor(d => d.OwnerID)
                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.CadastralPlanNumber</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.CadastralPlanNumber, new { @class = "form-control inputnumberwithhyphen", maxlength = 20, tabindex = 6 })
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@Html.Raw(@LocalResource.TerrainArea) <span class="text-danger">*</span></label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.Area, new { @class = "form-control inputdecimal required text-right", maxlength = 12, tabindex = 7, Value = (Model.Area > 0 ? Model.Area.Value.ToString(Common.DecimalPoints) : 0.ToString(Common.DecimalPoints)) })
                                    @Html.ValidationMessageFor(d => d.Area)
                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.Percentage <span class="text-danger">*</span></label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.Percentage, new { @class = "form-control inputdecimal text-right", maxlength = 12, tabindex = 8, Value = (Model.Percentage > 0 ? Model.Percentage.Value.ToString(Common.DecimalPoints) : 0.ToString(Common.DecimalPoints)) })
                                    @Html.ValidationMessageFor(d => d.Percentage)
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@Html.Raw(@LocalResource.TerrainValue)</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.TerrainValue, new { @class = "form-control text-right", @readonly = true, Value = (Model.TerrainValue > 0 ? Model.TerrainValue.Value.ToString(Common.DecimalPoints) : 0.ToString(Common.DecimalPoints)) })
                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.TotalTerrainValue</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.TotalTerrainValue, new { @class = "form-control text-right", @readonly = true, Value = (Model.TotalTerrainValue > 0 ? Model.TotalTerrainValue.Value.ToString(Common.DecimalPoints) : 0.ToString(Common.DecimalPoints)) })
                                </div>
                            </div>
                            <div class="row form-group">
                                @*<label class="col-sm-2 control-label">@LocalResource.TotalArea (<em>m</em> <sup>2</sup>)</label>
                                    <div class="col-sm-4">
                                        @Html.TextBoxFor(d => d.TotalArea, new { Value = String.Format("{0:" + Common.DecimalPoints + "}", Model.TotalArea), @class = "form-control inputdecimal text-right", maxlength = 12, @readonly = true, tabindex = 9 })
                                        @Html.ValidationMessageFor(d => d.TotalArea)
                                    </div>*@
                                <label class="col-sm-2 control-label">@LocalResource.Notes</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.Notes, new { @class = "form-control", maxlength = 500, tabindex = 10 })
                                </div>
                            </div>
                            @Html.HiddenFor(d => d.AccountPropertyConstructionDetailJson)
                        </div>
                    </div>
                    <div class="ibox-title ibox-title-with-button no-top-border">
                        <h5>@AccountResource.Address</h5>
                    </div>
                    <div class="ibox-content">
                        <div class="project-list form-horizontal">
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@AccountResource.AddressLine1</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.AddressLine1, new { @class = "form-control", maxlength = 500, tabindex = 11 })
                                </div>
                                <label class="col-sm-2 control-label">@AccountResource.AddressLine2</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.AddressLine2, new { @class = "form-control", maxlength = 500, tabindex = 12 })
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@GlobalResource.Country</label>
                                <div class="col-sm-4">
                                    @Html.DropDownListFor(d => d.CountryID, Model.CountryList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlCountry", tabindex = 13 })

                                </div>
                                <label class="col-sm-2 control-label">@AccountResource.State</label>
                                <div class="col-sm-4">
                                    @if (Model.CountryStateID > 0)
                                    {
                                        @Html.DropDownListFor(d => d.CountryStateID, Model.StateList, new { @class = "form-control select2dropdown", @id = "ddlState", tabindex = 14 })
                                    }
                                    else
                                    {
                                        @Html.DropDownListFor(d => d.CountryStateID, Model.StateList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlState", tabindex = 15 })
                                    }
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@AccountResource.City</label>
                                <div class="col-sm-4">
                                    @if (Model.CityID > 0)
                                    {
                                        @Html.DropDownListFor(d => d.CityID, Model.CityList, new { @class = "form-control select2dropdown", @id = "ddlCity", tabindex = 16 })
                                    }
                                    else
                                    {
                                        @Html.DropDownListFor(d => d.CityID, Model.CityList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlCity", tabindex = 17 })
                                    }
                                </div>
                                <label class="col-sm-2 control-label">@AccountResource.Town</label>
                                <div class="col-sm-4">
                                    @if (Model.TownID > 0)
                                    {
                                        @Html.DropDownListFor(d => d.TownID, Model.TownList, new { @class = "form-control select2dropdown", @id = "ddlTown", tabindex = 18 })
                                    }
                                    else
                                    {
                                        @Html.DropDownListFor(d => d.TownID, Model.TownList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlTown", tabindex = 19 })
                                    }
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@AccountResource.ZipCode</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.ZipPostalCode, new { @class = "form-control inputzipcode", @id = "txtZipCode", maxlength = 10, tabindex = 20 })
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="ibox-title ibox-title-with-button no-top-border">
                        <h5>@LocalResource.TerrainInformation</h5>
                    </div>
                    <div class="ibox-content">
                        <div class="project-list form-horizontal">
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@LocalResource.MainAccess</label>
                                <div class="col-sm-4">
                                    @Html.DropDownListFor(d => d.MainAccessID, Model.MainAccessList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlMainAccess", tabindex = 21 })

                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.PropertyFront</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.PropertyFront, new { Value = String.Format("{0:" + Common.DecimalPoints + "}", Model.PropertyFront), @class = "form-control inputdecimal text-right", maxlength = 12, tabindex = 22 })

                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@LocalResource.PropertyLength</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.PropertyLength, new { Value = String.Format("{0:" + Common.DecimalPoints + "}", Model.PropertyLength), @class = "form-control inputdecimal text-right", maxlength = 12, tabindex = 23 })
                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.Slope</label>
                                <div class="col-sm-4">
                                    @Html.DropDownListFor(d => d.SlopeID, Model.SlopeList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlSlope", tabindex = 24 })
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label"></label>
                                <div class="col-sm-4"></div>
                                <label class="col-sm-2 control-label">@LocalResource.SlopePercentage</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.SlopePercentage, new { @class = "form-control inputpercentage text-right", maxlength = 12, tabindex = 25, Value = (Model.SlopePercentage > 0 ? Model.SlopePercentage.Value.ToString(Common.DecimalPoints) : 0.ToString(Common.DecimalPoints)) })
                                    @Html.ValidationMessageFor(d => d.SlopePercentage)
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@LocalResource.Level</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.Level, new { Value = String.Format("{0:" + Common.DecimalPoints + "}", Model.Level), @class = "form-control inputdecimal text-right", maxlength = 12, tabindex = 26 })
                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.LevelType</label>
                                <div class="col-sm-4">
                                    @Html.DropDownListFor(d => d.LevelTypeID, Model.LevelTypeList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlLevelType", tabindex = 27 })
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@LocalResource.BlockLocation</label>
                                <div class="col-sm-4">
                                    @Html.DropDownListFor(d => d.BlockLocationID, Model.BlockLocationList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlBlockLocation", tabindex = 28 })

                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.Regular</label>
                                <div class="col-sm-4">
                                    @Html.DropDownListFor(d => d.RegularID, Model.RegularList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlRegular", tabindex = 29 })
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label"></label>
                                <div class="col-sm-4"></div>
                                <label class="col-sm-2 control-label">@LocalResource.FactorRegular</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.RegularFactor, new { @class = "form-control inputdecimal text-right", maxlength = 12, tabindex = 30, Value = (Model.RegularFactor > 0 ? Model.RegularFactor.Value.ToString(Common.DecimalPoints) : 0.ToString(Common.DecimalPoints)) })
                                    @Html.ValidationMessageFor(d => d.RegularFactor)
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@LocalResource.Hydrology</label>
                                <div class="col-sm-4">
                                    @Html.DropDownListFor(d => d.HydrologyID, Model.HydrologyList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlHydrology", tabindex = 31 })
                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.UseofLand</label>
                                <div class="col-sm-4">
                                    @Html.DropDownListFor(d => d.UseOfLandID, Model.UseOfLandList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlUseofLand", tabindex = 32 })
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@LocalResource.HomogeneousZone</label>
                                <div class="col-sm-4">
                                    @Html.DropDownListFor(d => d.HomogeneousZoneID, Model.HomogeneousZoneList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlHomogeneousZone", tabindex = 33 })
                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.Movement</label>
                                <div class="col-sm-4">
                                    @Html.DropDownListFor(d => d.MovementID, Model.MovementList, GlobalResource.DropDownSelectMessage, new { @class = "form-control select2dropdown", @id = "ddlMovement", tabindex = 34 })
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@LocalResource.Number</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.Number, new { @class = "form-control inputnumberwithhyphen", autocomplete = "off", ondragstart = "return false", ondrop = "return false", onpaste = "return false", maxlength = 50, tabindex = 35 })
                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.DateOfMovement</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(x => x.DateOfMovement, new { @class = "form-control inputdate", tabindex = 36, Value = (Model.DateOfMovement.HasValue ? Model.DateOfMovement.Value.ToString("d") : null) })
                                    @Html.ValidationMessageFor(d => d.DateOfMovement)
                                </div>
                            </div>
                            <div class="row form-group hide">
                                <label class="col-sm-2 control-label">@LocalResource.MapLocation</label>
                                <div class="col-sm-4">
                                    <button type="button" id="btnAddGeoLocation" class="btn btn-primary btn-sm" tabindex="37"><i class="fa fa-plus"></i> @GlobalResource.Add @LocalResource.MapLocation</button>
                                    @Html.HiddenFor(d => d.Latitude)
                                    @Html.HiddenFor(d => d.Longitude)
                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.CadastralLocation</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.CadastralLocationPath, new { @class = "form-control", maxlength = 5000, tabindex = 38 })
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@LocalResource.Services1</label>
                                <div class="col-sm-4">
                                    @Html.DropDownList("Services1", Model.PropertyServicesList1, new { @multiple = "multiple", @class = "form-control select2dropdown", tabindex = 39 })
                                    @Html.HiddenFor(d => d.ServicesList1)
                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.Services2</label>
                                <div class="col-sm-4">
                                    @Html.DropDownList("Services2", Model.PropertyServicesList2, new { @multiple = "multiple", @class = "form-control select2dropdown", tabindex = 40 })
                                    @Html.HiddenFor(d => d.ServicesList2)
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="ibox-title ibox-title-with-button no-top-border">
                        <h5>@LocalResource.Adjacent</h5>
                    </div>
                    <div class="ibox-content">
                        <div class="project-list form-horizontal">
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@LocalResource.North</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.North, new { @class = "form-control", maxlength = 500, tabindex = 41 })
                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.South</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.South, new { @class = "form-control", maxlength = 500, tabindex = 42 })
                                </div>
                            </div>
                            <div class="row form-group">
                                <label class="col-sm-2 control-label">@LocalResource.East</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.East, new { @class = "form-control", maxlength = 500, tabindex = 43 })
                                </div>
                                <label class="col-sm-2 control-label">@LocalResource.West</label>
                                <div class="col-sm-4">
                                    @Html.TextBoxFor(d => d.West, new { @class = "form-control", maxlength = 500, tabindex = 44 })
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="ibox-title no-top-border">
                        <div class="row">
                            <div class="col-lg-10 m-t">
                                <h4>@LocalResource.Constructions</h4>
                            </div>
                            <div class="col-lg-2 text-right m-t-xs">
                                <button class="btn btn-primary addConstruction" type="button" tabindex=45>@LocalResource.AddConstructions</button>
                            </div>
                        </div>
                    </div>
                    <div class="ibox-content">
                        <div class="Right-list">
                            <div class="table-responsive">
                                <table class="RightsTable footable table table-stripped toggle-arrow-tiny" data-page="false" data-sort="false" width="100%">
                                    <thead>
                                        <tr>
                                            <th data-toggle="true" width="20%">@GlobalResource.Type</th>
                                            <th width="20%">@LocalResource.ConstructionType</th>
                                            <th width="17%" class="text-right">@Html.Raw(@LocalResource.BuildingArea)</th>
                                            <th width="17%" class="text-right">@Html.Raw(@LocalResource.UnitValue)</th>
                                            <th width="16%" class="text-right">@LocalResource.BuildingTotalValue</th>
                                            <th width="10%"></th>
                                            <th data-hide="all">
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var objItem in Model.AccountPropertyConstructionDetail)
                                        {
                                            <tr data-id="@objItem.ID">
                                                <td class="table-description-field">@objItem.MaterialType</td>
                                                <td class="table-description-field">@Html.Raw(!string.IsNullOrEmpty(objItem.ConstructionType) ? objItem.ConstructionType : "-")</td>
                                                <td class="text-right">@objItem.BuildingArea.Value.ToString(Common.DecimalPoints)</td>
                                                <td class="text-right">@objItem.UnitValue.Value.ToString("C")</td>
                                                <td class="text-right">@objItem.TotalValue.Value.ToString("C")</td>
                                                <td class="text-right">
                                                    <a class='btn btn-white btn-sm btnCopyConstruction' data-type="old" data-id="@objItem.ID"><i class='fa fa-copy'></i></a>
                                                    <a class='btn btn-white btn-sm btnEditConstruction' data-type="old" data-id="@objItem.ID"><i class='fa fa-pencil'></i></a>
                                                    <a class='btn btn-white btn-sm btnDeleteConstruction' data-type="old" data-id="@objItem.ID"><i class='fa fa-trash'></i></a>
                                                </td>

                                                <td class="ConstructionList">
                                                    <div class="row1 border-bottom">
                                                        <table width="100%">
                                                            <tbody>
                                                                <tr data-id="@objItem.ID">
                                                                    <td class="col-lg-10">
                                                                        <table class="table no-borders">
                                                                            <tbody>
                                                                                <tr>
                                                                                    <td class="control-label text-right">
                                                                                        <label class="control-label">
                                                                                            @GlobalResource.Type :
                                                                                        </label>
                                                                                    </td>
                                                                                    <td class="text-left">
                                                                                        @objItem.MaterialType
                                                                                    </td>
                                                                                    <td class="control-label text-right">
                                                                                        <label class="control-label">
                                                                                            @LocalResource.CurrentAge :
                                                                                        </label>
                                                                                    </td>
                                                                                    <td class="text-left">
                                                                                        @if (objItem.TotalYears > 0)
                                                                                        {
                                                                                            @objItem.TotalYears
                                                                                        }
                                                                                        else
                                                                                        {
                                                                                            @Html.Raw('-')
                                                                                        }
                                                                                    </td>
                                                                                    <td class="control-label text-right">
                                                                                        <label class="control-label">
                                                                                            @LocalResource.State :
                                                                                        </label>
                                                                                    </td>
