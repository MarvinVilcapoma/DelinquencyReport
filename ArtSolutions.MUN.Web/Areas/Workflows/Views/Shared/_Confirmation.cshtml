
@using LocalResource = ArtSolutions.MUN.Web.Resources.Workflow
@using GLobalResource = ArtSolutions.MUN.Web.Resources.Global

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 text-right">
            <label>
                @GLobalResource.DocumentType
            </label>
        </div>
        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12">
            <label id="lblDocumentType"></label>
        </div>
    </div>
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 text-right">
            <label>
                @GLobalResource.Name
            </label>
        </div>
        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12">
            <label id="lblName"></label>
        </div>
    </div>
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 text-right">
            <label>
                @GLobalResource.Description
            </label>
        </div>
        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12">
            <label id="lblDescription"></label>
        </div>
    </div>
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="form-group">
            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 text-right">
                <label>
                    @GLobalResource.Status
                </label>
            </div>
            <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12">
                <div class="table-responsive">
                    <table class="table table-bordered white-bg" id="tblConfirmStatus">
                        <thead></thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

