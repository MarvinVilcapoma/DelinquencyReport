@using LocalResource = ArtSolutions.MUN.Web.Resources.Case
@using GlobalResource = ArtSolutions.MUN.Web.Resources.Global

@{
    ViewBag.Title = LocalResource.PrintCenterList;
}


<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>@GlobalResource.PrintCenter</h2>
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("Index", "Home", new {area = "" })">@GlobalResource.Home</a>
            </li>
            <li class="active">
                <strong>@ViewBag.Title</strong>
            </li>
        </ol>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="wrapper wrapper-content animated fadeInUp">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>@ViewBag.Title</h5>
                </div>
                <div class="ibox-content">
                    <div class="row m-b-lg m-t-sm">
                        <div class="col-lg-1 col-md-1 col-sm-1 col-xs-12">
                            <button type="button" id="btnrefresh" class="btn btn-white btn-sm" tabindex="3"><i class="fa fa-refresh"></i> @GlobalResource.Refresh</button>
                        </div>
                        <div class="col-lg-11 col-md-11 col-sm-11 col-xs-12">
                            <div class="input-group">
                                <input type="text" placeholder="@GlobalResource.TopSearchPlaceholder" class="input-sm form-control readdata" autofocus id="txtSearch" tabindex="1"> 
                                <span class="input-group-btn">
                                    <button type="button" class="btn btn-sm btn-primary" id="btnSearch" tabindex="2"> @GlobalResource.Go</button>
                                </span>
                            </div>
                        </div>
                    </div>
                    <table class="table table-stripped dataTables-example" id="ListTable"></table>
                </div>
            </div>
        </div>
    </div>
</div>
@section Styles {
    @Styles.Render("~/plugins/sweetAlertStyles")
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/plugins/dataTables")
    @Scripts.Render("~/plugins/sweetAlert")

    <script type="text/javascript">        
        var nodatamsg ='@Html.Raw(GlobalResource.NoDataMessage)';   
        var view ='@Html.Raw(GlobalResource.View)';  
        var edit ='@Html.Raw(GlobalResource.Edit)'; 
        var name ='@Html.Raw(GlobalResource.Name)'; 
        var keycode ='@Html.Raw(LocalResource.Key)';
        var actions ='@Html.Raw(GlobalResource.Actions)';  
        var template ='@Html.Raw(LocalResource.Template)'; 
        var format ='@Html.Raw(LocalResource.Format)'; 
        var user ='@Html.Raw(LocalResource.User)'; 
        var date ='@Html.Raw(GlobalResource.Date)';
        var cases ='@Html.Raw(GlobalResource.Cases)';
        var estimateAmount ='@Html.Raw(LocalResource.EstimateAmount)'; 
        var download ='@Html.Raw(GlobalResource.Download)'; 
        var print ='@Html.Raw(GlobalResource.Print)';
        var user ='@Html.Raw(LocalResource.User)'; 
        var first ='@Html.Raw(GlobalResource.First)';
        var last ='@Html.Raw(GlobalResource.Last)'; 
        var next ='@Html.Raw(GlobalResource.Next)'; 
        var previous ='@Html.Raw(GlobalResource.Previous)'; 
    </script>
    <script src="~/Scripts/app/printletterlist.js"></script>
}
